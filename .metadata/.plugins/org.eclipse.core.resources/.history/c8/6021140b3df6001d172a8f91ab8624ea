package entities;

import javafx.scene.image.Image;

public class Enemy {
	Image idle = new Image(getClass().getResourceAsStream("/Flight.png"));
	ImageView animation = new ImageView(getClass().getResourceAsStream("/Flight.png"));


	public void loadAnimations() {
		for(int i=0;i<animation.length;i++) {
			animation[i] = idle.
		}
	}
}
